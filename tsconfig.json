{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,  
    "strict": true,  
    "baseUrl": ".",
    "paths": {
      "@/*": ["./app/*"]
    },
    "jsx": "react-jsx", 
    "target": "es5",  
    "moduleResolution": "node",
    "esModuleInterop": true,  
    "skipLibCheck": true,  
    "allowJs": true  
  }
}
